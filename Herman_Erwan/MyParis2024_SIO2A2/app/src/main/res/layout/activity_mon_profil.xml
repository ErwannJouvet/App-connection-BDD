<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MonProfil">

    <EditText
        android:id="@+id/idTel"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:background="@android:drawable/button_onoff_indicator_on"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="514dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="140dp"
        android:layout_height="42dp"
        android:text="Téléphone : "
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="513dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="117dp"
        android:layout_height="46dp"
        android:text="MDP : "
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="437dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="117dp"
        android:layout_height="46dp"
        android:text="Email : "
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="342dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="117dp"
        android:layout_height="46dp"
        android:text="PRENOM : "
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="254dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="415dp"
        android:layout_height="119dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/log"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="92dp"
        android:layout_height="44dp"
        android:text="NOM : "
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="173dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/idNom"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:background="@android:drawable/button_onoff_indicator_on"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="173dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/idPrenom"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:background="@android:drawable/button_onoff_indicator_on"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="254dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/idEmail2"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:background="@android:drawable/button_onoff_indicator_on"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="342dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/idMdp2"
        android:layout_width="208dp"
        android:layout_height="45dp"
        android:background="@android:drawable/button_onoff_indicator_on"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="437dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/idMiseAJour"
        android:layout_width="411dp"
        android:layout_height="57dp"
        android:text="Mise à jour et retour "
        android:textSize="18sp"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="616dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="412dp"
        android:layout_height="45dp"
        android:background="#C05B5B"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>